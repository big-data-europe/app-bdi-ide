<http://stack-builder.big-data-europe.eu/resources/docker-composes/58CFDE700675FB0009000001>	ns3:title	"pilot-sc4-cycle1" ;
	ns1:text	"# run zookeeper and kafka as docker services\nversion: '2'\nservices:\n    zookeeper:\n        build:\n            context: images/sc4-zookeeper\n        image: sc4-zookeeper\n        environment:\n            - MYID=1\n            - DELAY=0\n        ports:\n            - 2181:2181\n    kafka-broker:\n        build:\n            context: images/sc4-kafka-broker\n        image: sc4-kafka-broker\n        environment:\n            - TOPICS=taxi,testtopic\n            - ZOOKEEPER_SERVERS=zookeeper:2181\n        links:\n            - zookeeper\n        ports:\n            - 9092:9092\n    kafka-maketopic:\n        build:\n            context: images/sc4-kafka-maketopic\n        image: sc4-kafka-maketopic\n        environment:\n            - ZOOKEEPER_SERVERS=zookeeper:2181\n            - KAFKA_BROKER=kafka-broker:9092\n        links:\n            - kafka-broker\n    kafka-cli-producer:\n        build:\n            context: images/sc4-kafka-cli-producer\n        image: sc4-kafka-producer\n        environment:\n            - ZOOKEEPER_SERVERS=zookeeper:2181\n            - KAFKA_BROKER_LIST=kafka-broker:9092\n        links:\n            - kafka-broker\n    kafka-cli-consumer:\n        build:\n            context: images/sc4-kafka-cli-consumer\n        image: sc4-kafka-consumer\n        environment:\n            - ZOOKEEPER_SERVERS=zookeeper:2181\n        links:\n            - kafka-broker\n    pilot-sc4-fcd-producer:\n        image: datiaperti/pilot-sc4-fcd-producer:v0.0.1\n        environment:\n            - ZOOKEEPER_SERVERS=zookeeper:2181\n        links:\n            - kafka-broker:kafkahost" .
<http://stack-builder.big-data-europe.eu/resources/docker-composes/58CFDE740675FB0009000002>	rdf:type	ns1:DockerCompose ;
	ns2:uuid	"58CFDE740675FB0009000002" ;
	ns3:title	"pilot-sc6-cycle2" ;
	ns1:text	"version: \"2\"\n\n#server.1=sc6_zoo_1:31200:31201:participant;31202\n#server.2=sc6_zoo_2:31200:31201:participant;31202\n#server.3=sc6_zoo_3:31200:31201:participant;31202\n\nservices:\n  sc6_zoo_1:\n    image: \"bde2020/sc6-zookeeper\"\n    ports: \n      - 31200:31200\n      - 31201:31201\n      - 31202:31202\n    container_name: sc6_zoo_1\n    command: \"bash -c /startup\"\n    hostname: \"sc6_zoo_1\"\n    environment:\n      - \"constraint:node==akswnc4.aksw.internal\"\n  sc6_zoo_2:\n    image: \"bde2020/sc6-zookeeper\"\n    ports:\n      - 31200:31200\n      - 31201:31201\n      - 31202:31202\n    container_name: sc6_zoo_2\n    command: \"bash -c /startup\"\n    hostname: \"sc6_zoo_2\"\n    environment:\n      - \"constraint:node==akswnc5.aksw.internal\"\n  sc6_zoo_3:\n    image: \"bde2020/sc6-zookeeper\"\n    ports:\n      - 31200:31200\n      - 31201:31201\n      - 31202:31202\n    container_name: sc6_zoo_3\n    command: \"bash -c /startup\"\n    hostname: \"sc6_zoo_3\"\n    environment:\n      - \"constraint:node==akswnc6.aksw.internal\"\n  sc6-kafka-1:\n    image: \"bde2020/sc6-kafka\"\n    depends_on:\n      - sc6_zoo_1\n      - sc6_zoo_2\n      - sc6_zoo_3\n    ports:\n      - 9092:9092\n    command: \"bash -c /app/bin/kafka-init\"\n    hostname: \"sc6-kafka-1\"\n    environment:\n      - \"constraint:node==akswnc4.aksw.internal\"\n  sc6-kafka-2:\n    image: \"bde2020/sc6-kafka\"\n    depends_on:\n      - sc6_zoo_1\n      - sc6_zoo_2\n      - sc6_zoo_3\n    ports:\n      - 9092:9092\n    command: \"bash -c /app/bin/kafka-init\"\n    hostname: \"sc6-kafka-2\"\n    environment:\n      - \"constraint:node==akswnc5.aksw.internal\"\n  sc6-kafka-3:\n    image: \"bde2020/sc6-kafka\"\n    depends_on:\n      - sc6_zoo_1\n      - sc6_zoo_2\n      - sc6_zoo_3\n    ports:\n      - 9092:9092\n    command: \"bash -c /app/bin/kafka-init\"\n    hostname: \"sc6-kafka-3\"\n    environment:\n      - \"constraint:node==akswnc6.aksw.internal\"\n  sc6-flume:\n    image: \"bde2020/sc6-flume\"\n    depends_on:\n      - sc6_zoo_1\n      - sc6_zoo_2\n      - sc6_zoo_3\n      - sc6-kafka-1\n      - sc6-kafka-2\n      - sc6-kafka-3\n    command: \"bash -c /app/bin/flume-init\"\n    volumes:\n      - ./data:/var/lib/bde/flume/sc6/budgets\n    environment:\n      - \"constraint:node==akswnc4.aksw.internal\"\n  sc6-spark-master:\n    image: bde2020/spark-master:2.1.0-hadoop2.7\n    container_name: sc6-spark-master\n    ports:\n      - \"58080:8080\"\n      - \"57077:7077\"\n    environment:\n      - \"constraint:node==akswnc4.aksw.internal\"\n  sc6-spark-worker-1:\n    image: bde2020/spark-worker:2.1.0-hadoop2.7\n    depends_on:\n      - sc6-spark-master\n    ports:\n      - \"58081:8081\"\n    environment:\n      - \"SPARK_MASTER=spark://sc6-spark-master:7077\"\n      - \"constraint:node==akswnc4.aksw.internal\"\n  sc6-spark-worker-2:\n    image: bde2020/spark-worker:2.1.0-hadoop2.7\n    depends_on:\n      - sc6-spark-master\n    ports:\n      - \"58081:8081\"\n    environment:\n      - \"SPARK_MASTER=spark://sc6-spark-master:7077\"\n      - \"constraint:node==akswnc5.aksw.internal\"\n  sc6-spark-worker-3:\n    image: bde2020/spark-worker:2.1.0-hadoop2.7\n    depends_on:\n      - sc6-spark-master\n    ports:\n      - \"58081:8081\"\n    environment:\n      - \"SPARK_MASTER=spark://sc6-spark-master:7077\"\n      - \"constraint:node==akswnc6.aksw.internal\"\n  sc6-spark-job:\n    image: bde2020/sc6-spark-submit\n    depends_on:\n      - sc6-spark-master\n      - sc6-spark-worker-1\n      - sc6-spark-worker-2\n      - sc6-spark-worker-3\n    environment:\n      - \"constraint:node==akswnc4.aksw.internal\"" .
<http://stack-builder.big-data-europe.eu/resources/docker-composes/58CFDE750675FB0009000003>	rdf:type	ns1:DockerCompose ;
