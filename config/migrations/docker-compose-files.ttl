	ns3:title	"Stack Builder" ;
	ns1:text	"version: \"2\"\nservices:\n  frontend:\n    image: bde2020/ember-stack-builder-frontend:0.1.0\n    ports:\n      - \"88:80\"\n    links:\n      - identifier:backend\n  identifier:\n    image: semtech/mu-identifier:1.2.0\n    links:\n      - dispatcher:dispatcher\n  dispatcher:\n    image: semtech/mu-dispatcher:1.1.0\n    links:\n      - resource:resource\n    volumes:\n      - ./config/dispatcher:/config\n    ports:\n      - \"444:80\"\n  db:\n    image: tenforce/virtuoso:1.1.0-virtuoso7.2.4\n    environment:\n      SPARQL_UPDATE: \"true\"\n      DEFAULT_GRAPH: \"http://mu.semte.ch/application\"\n    ports:\n      - \"8890:8890\"\n    volumes:\n      - ./data/db:/data\n  resource:\n    image: semtech/mu-cl-resources\n    links:\n      - db:database\n    volumes:\n      - ./config/resources:/config\n    ports:\n      - \"555:80\"\n  backend:\n    image: bde2020/mu-docker-compose-file-service:0.1.0\n    links:\n      - db:database\n    ports:\n      - \"666:80\"\n" .
<http://stack-builder.big-data-europe.eu/resources/docker-composes/58CFDE760675FB0009000004>	rdf:type	ns1:DockerCompose ;
	ns2:uuid	"58CFDE760675FB0009000004" ;
	ns3:title	"pilot-sc7-change-detector" ;
	ns1:text	"version: '2'\nservices:\n\n  lookupservice:\n    image: gioargyr/lookup-service:1.0.0\n    container_name: lookupservice\n    ports:\n      - \"8190:8080\"\n    networks:\n      - default\n\n  imageaggregator:\n    image: gioargyr/image-aggregator:1.0.0\n    container_name: imageaggregator\n    volumes:\n      - /home/iitadmin/imageaggregator:/images\n    ports:\n      - \"12345:8080\"\n    networks:\n      - default\n\n  postgis:\n    image: bde2020/postgis\n    container_name: postgis\n    volumes:\n      - /srv/postgis:/var/lib/postgresql/9.4/main\n    ports:\n      - \"5432:5432\"\n    networks:\n      - default\n\n  strabon:\n    image: bde2020/strabon\n    container_name: strabon\n    ports:\n      - \"8191:8080\"\n    depends_on:\n      - postgis\n    networks:\n      - default\n    \n  semagrow:\n    image: semagrow/semagrow-cassandra\n    container_name: semagrow_bde\n    ports:\n      - \"8192:8080\"\n    volumes:\n      - ./config/semagrow:/etc/default/semagrow\n    depends_on:\n      - strabon\n    networks:\n      - default\n      \n  geotriples:\n    image: gioargyr/docker-geotriples-ws:1.0.0\n    container_name: geotriples\n    ports:\n      - \"8193:8080\"\n    depends_on:\n      - strabon\n    networks:\n      - default\n\n  master:\n    image: bde2020/spark-master:1.5.1-hadoop2.6\n    hostname: spark-master\n    container_name: spark-master\n    environment:\n      - \"constraint:node==master\"\n      - INIT_DAEMON_STEP:spark_master_init\n      - VIRTUAL_HOST:spark-master.iit.demokritos.gr\n      - VIRTUAL_PORT:8080\n    ports:\n      - \"8290:8080\"\n    volumes:\n      - ./data/spark-master:/data\n    networks:\n      - default\n\n  worker1:\n    image: bde2020/spark-worker:1.5.1-hadoop2.6\n    container_name: worker1\n    environment:\n      - \"constraint:node==slave1\"\n      - VIRTUAL_HOST:spark-worker1.iit.demokritos.gr\n      - VIRTUAL_PORT:8081\n    depends_on:\n      - master\n    networks:\n      - default\n\n  worker2:\n    image: bde2020/spark-worker:1.5.1-hadoop2.6\n    container_name: worker2\n    environment:\n      - \"constraint:node==slave2\"\n      - VIRTUAL_HOST:spark-worker2.iit.demokritos.gr\n      - VIRTUAL_PORT:8082\n    depends_on:\n      - master\n    networks:\n      - default\n\n  worker3:\n    image: bde2020/spark-worker:1.5.1-hadoop2.6\n    container_name: worker3\n    environment:\n      - \"constraint:node==slave3\"\n      - VIRTUAL_HOST:spark-worker3.iit.demokritos.gr\n      - VIRTUAL_PORT:8083\n    depends_on:\n      - master\n    networks:\n      - default\n\n  worker4:\n    image: bde2020/spark-worker:1.5.1-hadoop2.6\n    container_name: worker4\n    environment:\n      - \"constraint:node==slave4\"\n      - VIRTUAL_HOST:spark-worker4.iit.demokritos.gr\n      - VIRTUAL_PORT:8084\n    depends_on:\n      - master\n    networks:\n      - default\n\n  changedetector:\n    image: gioargyr/pilot-sc7-change-detector:1.0.0\n    container_name: changedetector\n    environment:\n      - INIT_DAEMON_STEP:detect_changes\n    volumes:\n      - /home/iitadmin/imageaggregator:/inputimages\n    depends_on:\n      - master\n    networks:\n      - default\n\n  integratorui:\n    image: bde2020/integrator-ui:latest\n    container_name: integratorui\n    environment:\n      - VIRTUAL_HOST:iit.demokritos.gr\n    ports:\n      - \"8390:80\"\n    volumes:\n      - ./config/integrator:/app/config\n    networks:\n      - default\n\n  dispatcher:\n    image: semtech/mu-dispatcher:1.0.1\n    container_name: dispatcher\n    volumes:\n      - ./config:/config\n    networks:\n      - default\n\n  identifier:\n    image: semtech/mu-identifier:1.0.0\n    container_name: identifier\n    environment:\n      - \"constraint:node==slave2\"\n    networks:\n      - default\n\n  monitor:\n    image: bde2020/pipeline-monitor-frontend:0.1.0\n    container_name: monitor\n    environment:\n      - VIRTUAL_HOST:monitor.iit.demokritos.gr\n    ports:\n      - \"8391:80\"\n    links:\n      - identifier:backend\n    networks:\n      - default\n\n  initdaemon:\n    image: bde2020/mu-init-daemon-service:0.1.0\n    container_name: initdaemon\n    networks:\n      - default\n\n  pipeline:\n    image: bde2020/mu-pipeline-service:0.1.0\n    container_name: pipeline\n    networks:\n      - default\n\n  database:\n    image: tenforce/virtuoso:1.0.0-virtuoso7.2.2\n    container_name: database\n    environment:\n      - SPARQL_UPDATE:\"true\"\n      - DEFAULT_GRAPH:http://mu.semte.ch/application\n      - VIRTUAL_HOST:virtuoso.iit.demokritos.gr\n      - VIRTUAL_PORT:8890\n    volumes:\n      - ./data/db/toLoad:/data\n    ports:\n      - \"8890:8890\"\n    networks:\n      - default\n\nnetworks:\n  default:\n    external:\n      name: bde_net" .
<http://stack-builder.big-data-europe.eu/resources/docker-composes/58EDD9C785AE3C0009000001>	rdf:type	ns1:DockerCompose ;
	ns2:uuid	"58EDD9C785AE3C0009000001" ;
	ns3:title	"demo-spark-sensor-data" ;
	ns1:text	"version: '2'\n\nservices:\n  ###\n  # Spark sensor demo pipeline\n  ###\n  namenode:\n    image: bde2020/hadoop-namenode:1.0.0\n    hostname: namenode\n    volumes:\n      - ./data/hadoop/namenode:/hadoop/dfs/name\n    expose:\n      - \"50070\"\n    environment:\n      CLUSTER_NAME: test\n      INIT_DAEMON_STEP: setup_hdfs\n      VIRTUAL_HOST: hdfs-namenode.demo.big-data-europe.local\n    env_file:\n      - ./config/hadoop/hadoop.env\n  resourcemanager:\n    image: bde2020/hadoop-resourcemanager:1.0.0\n    hostname: resourcemanager\n    expose:\n      - \"8031\"\n      - \"8088\"\n    environment:\n      VIRTUAL_HOST: hdfs-resourcemanager.demo.big-data-europe.local\n      VIRTUAL_PORT: 8088\n    env_file:\n      - ./config/hadoop/hadoop.env\n  historyserver:\n    image: bde2020/hadoop-historyserver:1.0.0\n    hostname: historyserver\n    volumes:\n      - ./data/hadoop/historyserver:/hadoop/yarn/timeline\n    env_file:\n      - ./config/hadoop/hadoop.env\n  nodemanager:\n    image: bde2020/hadoop-nodemanager:1.0.0\n    hostname: nodemanager\n    expose:\n      - \"8042\"\n    environment:\n      VIRTUAL_HOST: hdfs-nodemanager.demo.big-data-europe.local\n    env_file:\n      - ./config/hadoop/hadoop.env\n  datanode:\n    image: bde2020/hadoop-datanode:1.0.0\n    hostname: datanode\n    env_file:\n      - ./config/hadoop/hadoop.env\n  filebrowser:\n    image: bde2020/hdfs-filebrowser\n    hostname: filebrowser\n    environment:\n      NAMENODE_HOST: namenode\n      VIRTUAL_HOST: hue.demo.big-data-europe.local\n      VIRTUAL_PORT: 8088\n  master:\n    image: bde2020/spark-master:1.5.1-hadoop2.6\n    hostname: spark-master\n    environment:\n      INIT_DAEMON_STEP: setup_spark\n      VIRTUAL_HOST: spark-master.demo.big-data-europe.local\n      VIRTUAL_PORT: 8080\n    volumes:\n      - ./data/spark-master:/data\n  worker:\n    image: bde2020/spark-worker:1.5.1-hadoop2.6\n    links:\n      - \"master:spark-master\"\n    environment:\n      VIRTUAL_HOST: spark-worker.demo.big-data-europe.local\n      VIRTUAL_PORT: 8081\n  demo:\n    image: bde2020/demo-spark-sensor-data:2.0.0\n    environment:\n      INIT_DAEMON_STEP: compute_aggregations\n      HDFS_URL: hdfs://namenode:8020\n    links:\n      - \"master:spark-master\"\n  integratorui:\n    image: bde2020/integrator-ui:latest\n    volumes:\n      - ./config/integrator:/app/config\n    environment:\n      VIRTUAL_HOST: demo.big-data-europe.local\n  csswrapper:\n    image: madnificent/demo-spark-sensor-data-integrator-css-wrapper\n    ports:\n      - 80:80\n    links:\n      - namenode:namenode\n      - resourcemanager:resourcemanager\n      - nodemanager:nodemanager\n      - filebrowser:filebrowser\n      - master:master\n      - worker:worker\n      - integratorui:integratorui\n      - monitor:monitor\n      - database:database\n    depends_on:\n      - namenode\n      - resourcemanager\n      - nodemanager\n      - filebrowser\n      - master\n      - worker\n      - integratorui\n      - monitor\n      - database\n  ###\n  # mu.semte.ch stack\n  ###\n  monitor:\n    image: bde2020/pipeline-monitor-frontend:0.1.0\n    links:\n      - identifier:backend\n    environment:\n      VIRTUAL_HOST: monitor.demo.big-data-europe.local\n  identifier:\n    image: semtech/mu-identifier:1.0.0\n  dispatcher:\n    image: semtech/mu-dispatcher:1.0.1\n    volumes:\n      - ./config:/config\n  database:\n    image: tenforce/virtuoso:1.0.0-virtuoso7.2.2\n    environment:\n      SPARQL_UPDATE: \"true\"\n      DEFAULT_GRAPH: http://mu.semte.ch/application\n      VIRTUAL_HOST: virtuoso.demo.big-data-europe.local\n      VIRTUAL_PORT: 8890\n    volumes:\n      - ./data/db:/data\n  pipeline:\n    image: bde2020/mu-pipeline-service:0.1.0\n  initdaemon:\n    image: bde2020/mu-init-daemon-service:0.1.0\n# networks:\n#   default:\n#     driver: overlay\n" .
